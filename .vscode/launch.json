    "version": "0.2.0",
    "configurations": [
        {
            "name": "ILSpy",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ILSpy/bin/Debug/net10.0-windows/ILSpy.exe",
            "args": ["--newinstance"],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}